<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="cn.xyh.a_hqlQuery">

    <class userName="Employee" table="t_employee">
        <id userName="empId">
            <generator class="native"></generator>
        </id>
        <property userName="empName"></property>
        <property userName="salary"></property>
        <many-to-one userName="dept" class="Dept" column="dept_id"></many-to-one>
    </class>

</hibernate-mapping>