<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="cn.xyh.c_many2manyMapping">

    <class userName="Developer" table="t_developer">
        <id userName="d_id">
            <generator class="native"></generator>
        </id>
        <property userName="d_name"></property>
        <set userName="projects" table="t_relation">
            <key column="did"></key>
            <many-to-many column="proId" class="Project"></many-to-many>
        </set>
    </class>

</hibernate-mapping>