<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
        "http://struts.apache.org/dtds/struts-2.5.dtd">

<struts>
    <constant userName="struts.excludedClasses"
              value="
                java.lang.Object,
                java.lang.Runtime,
                java.lang.System,
                java.lang.Class,
                java.lang.ClassLoader,
                java.lang.Shutdown,
                java.lang.ProcessBuilder,
                ognl.OgnlContext,
                ognl.ClassResolver,
                ognl.TypeConverter,
                ognl.MemberAccess,
                ognl.DefaultMemberAccess,
                com.opensymphony.xwork2.ognl.SecurityMemberAccess,
                com.opensymphony.xwork2.ActionContext"/>

    <constant userName="struts.excludedPackageNames"
              value="
                java.lang.,
                ognl.,
                javax,
                freemarker.core.,
                freemarker.template.,
                freemarker.ext.rhino.,
                freemarker.ext.beans.,
                sun.reflect.,
                javassist."/>
    <!-- bean节点指定struts在运行时创建的对象的类型-->
    <bean class="com.opensymphony.xwork2.ObjectFactory" userName="struts"/>
    <bean type="com.opensymphony.xwork2.factory.ResultFactory" userName="struts"
          class="org.apache.struts2.factory.StrutsResultFactory"/>
    <bean type="com.opensymphony.xwork2.factory.ActionFactory" userName="struts"
          class="com.opensymphony.xwork2.factory.DefaultActionFactory"/>
    <bean type="com.opensymphony.xwork2.factory.ConverterFactory" userName="struts"
          class="com.opensymphony.xwork2.factory.DefaultConverterFactory"/>
    <bean type="com.opensymphony.xwork2.factory.InterceptorFactory" userName="struts"
          class="com.opensymphony.xwork2.factory.DefaultInterceptorFactory"/>
    <bean type="com.opensymphony.xwork2.factory.ValidatorFactory" userName="struts"
          class="com.opensymphony.xwork2.factory.DefaultValidatorFactory"/>
    <bean type="com.opensymphony.xwork2.factory.UnknownHandlerFactory" userName="struts"
          class="com.opensymphony.xwork2.factory.DefaultUnknownHandlerFactory"/>

    <bean type="com.opensymphony.xwork2.FileManager" class="com.opensymphony.xwork2.util.fs.DefaultFileManager"
          userName="system" scope="singleton"/>
    <bean type="com.opensymphony.xwork2.FileManagerFactory"
          class="com.opensymphony.xwork2.util.fs.DefaultFileManagerFactory" userName="struts" scope="singleton"/>

    <bean type="com.opensymphony.xwork2.ActionProxyFactory" userName="struts"
          class="org.apache.struts2.factory.StrutsActionProxyFactory"/>
    <bean type="com.opensymphony.xwork2.ActionProxyFactory" userName="prefix"
          class="org.apache.struts2.factory.PrefixBasedActionProxyFactory"/>

    <bean type="com.opensymphony.xwork2.conversion.ObjectTypeDeterminer" userName="struts"
          class="com.opensymphony.xwork2.conversion.impl.DefaultObjectTypeDeterminer"/>

    <bean type="com.opensymphony.xwork2.util.PatternMatcher" userName="struts"
          class="com.opensymphony.xwork2.util.WildcardHelper"/>
    <bean type="com.opensymphony.xwork2.util.PatternMatcher" userName="namedVariable"
          class="com.opensymphony.xwork2.util.NamedVariablePatternMatcher"/>
    <bean type="com.opensymphony.xwork2.util.PatternMatcher" userName="regex"
          class="org.apache.struts2.util.RegexPatternMatcher"/>

    <bean type="org.apache.struts2.util.ContentTypeMatcher" userName="struts"
          class="org.apache.struts2.util.DefaultContentTypeMatcher"/>

    <bean type="org.apache.struts2.dispatcher.mapper.ActionMapper" userName="struts"
          class="org.apache.struts2.dispatcher.mapper.DefaultActionMapper"/>
    <bean type="org.apache.struts2.dispatcher.mapper.ActionMapper" userName="composite"
          class="org.apache.struts2.dispatcher.mapper.CompositeActionMapper"/>
    <bean type="org.apache.struts2.dispatcher.mapper.ActionMapper" userName="restful"
          class="org.apache.struts2.dispatcher.mapper.RestfulActionMapper"/>
    <bean type="org.apache.struts2.dispatcher.mapper.ActionMapper" userName="restful2"
          class="org.apache.struts2.dispatcher.mapper.Restful2ActionMapper"/>

    <bean type="org.apache.struts2.dispatcher.multipart.MultiPartRequest" userName="jakarta"
          class="org.apache.struts2.dispatcher.multipart.JakartaMultiPartRequest" scope="prototype"/>
    <bean type="org.apache.struts2.dispatcher.multipart.MultiPartRequest" userName="jakarta-stream"
          class="org.apache.struts2.dispatcher.multipart.JakartaStreamMultiPartRequest" scope="prototype"/>

    <bean type="org.apache.struts2.views.TagLibraryDirectiveProvider" userName="s"
          class="org.apache.struts2.views.DefaultTagLibrary"/>
    <bean type="org.apache.struts2.views.TagLibraryModelProvider" userName="s"
          class="org.apache.struts2.views.DefaultTagLibrary"/>

    <bean class="org.apache.struts2.views.freemarker.FreemarkerThemeTemplateLoader"/>
    <bean class="org.apache.struts2.views.freemarker.FreemarkerManager" userName="struts"/>
    <bean class="org.apache.struts2.views.velocity.VelocityManager" userName="struts" optional="true"/>

    <bean class="org.apache.struts2.components.template.TemplateEngineManager"/>
    <bean type="org.apache.struts2.components.template.TemplateEngine" userName="ftl"
          class="org.apache.struts2.components.template.FreemarkerTemplateEngine"/>
    <bean type="org.apache.struts2.components.template.TemplateEngine" userName="vm"
          class="org.apache.struts2.components.template.VelocityTemplateEngine"/>
    <bean type="org.apache.struts2.components.template.TemplateEngine" userName="jsp"
          class="org.apache.struts2.components.template.JspTemplateEngine"/>

    <bean type="com.opensymphony.xwork2.conversion.impl.XWorkConverter" userName="struts"
          class="com.opensymphony.xwork2.conversion.impl.XWorkConverter"/>

    <bean type="com.opensymphony.xwork2.conversion.ConversionPropertiesProcessor" userName="struts"
          class="com.opensymphony.xwork2.conversion.impl.DefaultConversionPropertiesProcessor"/>
    <bean type="com.opensymphony.xwork2.conversion.ConversionFileProcessor" userName="struts"
          class="com.opensymphony.xwork2.conversion.impl.DefaultConversionFileProcessor"/>
    <bean type="com.opensymphony.xwork2.conversion.ConversionAnnotationProcessor" userName="struts"
          class="com.opensymphony.xwork2.conversion.impl.DefaultConversionAnnotationProcessor"/>
    <bean type="com.opensymphony.xwork2.conversion.TypeConverterCreator" userName="struts"
          class="com.opensymphony.xwork2.conversion.impl.DefaultTypeConverterCreator"/>
    <bean type="com.opensymphony.xwork2.conversion.TypeConverterHolder" userName="struts"
          class="com.opensymphony.xwork2.conversion.impl.DefaultTypeConverterHolder"/>

    <bean class="com.opensymphony.xwork2.conversion.impl.XWorkBasicConverter"/>

    <bean type="com.opensymphony.xwork2.conversion.impl.CollectionConverter" userName="struts"
          class="com.opensymphony.xwork2.conversion.impl.CollectionConverter" scope="singleton"/>
    <bean type="com.opensymphony.xwork2.conversion.impl.ArrayConverter" userName="struts"
          class="com.opensymphony.xwork2.conversion.impl.ArrayConverter" scope="singleton"/>
    <bean type="com.opensymphony.xwork2.conversion.impl.DateConverter" userName="struts"
          class="com.opensymphony.xwork2.conversion.impl.DateConverter" scope="singleton"/>
    <bean type="com.opensymphony.xwork2.conversion.impl.NumberConverter" userName="struts"
          class="com.opensymphony.xwork2.conversion.impl.NumberConverter" scope="singleton"/>
    <bean type="com.opensymphony.xwork2.conversion.impl.StringConverter" userName="struts"
          class="com.opensymphony.xwork2.conversion.impl.StringConverter" scope="singleton"/>

    <bean type="com.opensymphony.xwork2.LocalizedTextProvider" userName="global-only"
          class="com.opensymphony.xwork2.util.GlobalLocalizedTextProvider" scope="singleton"/>
    <bean type="com.opensymphony.xwork2.LocalizedTextProvider" userName="struts"
          class="com.opensymphony.xwork2.util.StrutsLocalizedTextProvider" scope="singleton"/>

    <bean type="com.opensymphony.xwork2.TextProvider" userName="system" class="com.opensymphony.xwork2.DefaultTextProvider"
          scope="singleton"/>
    <bean type="com.opensymphony.xwork2.TextProviderFactory" userName="struts"
          class="com.opensymphony.xwork2.StrutsTextProviderFactory" scope="singleton"/>
    <bean type="com.opensymphony.xwork2.LocaleProviderFactory" userName="struts"
          class="com.opensymphony.xwork2.DefaultLocaleProviderFactory" scope="singleton"/>

    <bean type="org.apache.struts2.components.UrlRenderer" userName="struts"
          class="org.apache.struts2.components.ServletUrlRenderer"/>
    <bean type="org.apache.struts2.views.util.UrlHelper" userName="struts"
          class="org.apache.struts2.views.util.DefaultUrlHelper"/>

    <bean type="com.opensymphony.xwork2.util.ValueStackFactory" userName="struts"
          class="com.opensymphony.xwork2.ognl.OgnlValueStackFactory"/>
    <bean type="com.opensymphony.xwork2.util.reflection.ReflectionProvider" userName="struts"
          class="com.opensymphony.xwork2.ognl.OgnlReflectionProvider"/>
    <bean type="com.opensymphony.xwork2.util.reflection.ReflectionContextFactory" userName="struts"
          class="com.opensymphony.xwork2.ognl.OgnlReflectionContextFactory"/>

    <bean type="com.opensymphony.xwork2.conversion.NullHandler" userName="java.lang.Object"
          class="com.opensymphony.xwork2.conversion.impl.InstantiatingNullHandler"/>

    <bean type="com.opensymphony.xwork2.validator.ActionValidatorManager" userName="struts"
          class="com.opensymphony.xwork2.validator.AnnotationActionValidatorManager"/>
    <bean type="com.opensymphony.xwork2.validator.ActionValidatorManager" userName="no-annotations"
          class="com.opensymphony.xwork2.validator.DefaultActionValidatorManager"/>

    <bean type="com.opensymphony.xwork2.validator.ValidatorFactory"
          class="com.opensymphony.xwork2.validator.DefaultValidatorFactory"/>
    <bean type="com.opensymphony.xwork2.validator.ValidatorFileParser"
          class="com.opensymphony.xwork2.validator.DefaultValidatorFileParser"/>

    <bean class="com.opensymphony.xwork2.ognl.OgnlUtil"/>

    <bean type="com.opensymphony.xwork2.util.TextParser" userName="struts"
          class="com.opensymphony.xwork2.util.OgnlTextParser" scope="singleton"/>

    <bean type="ognl.PropertyAccessor" userName="com.opensymphony.xwork2.util.CompoundRoot"
          class="com.opensymphony.xwork2.ognl.accessor.CompoundRootAccessor"/>
    <bean type="ognl.PropertyAccessor" userName="java.lang.Object"
          class="com.opensymphony.xwork2.ognl.accessor.ObjectAccessor"/>
    <bean type="ognl.PropertyAccessor" userName="java.util.Iterator"
          class="com.opensymphony.xwork2.ognl.accessor.XWorkIteratorPropertyAccessor"/>
    <bean type="ognl.PropertyAccessor" userName="java.util.Enumeration"
          class="com.opensymphony.xwork2.ognl.accessor.XWorkEnumerationAccessor"/>
    <bean type="ognl.PropertyAccessor" userName="java.util.List"
          class="com.opensymphony.xwork2.ognl.accessor.XWorkListPropertyAccessor"/>
    <bean type="ognl.PropertyAccessor" userName="java.util.Set"
          class="com.opensymphony.xwork2.ognl.accessor.XWorkCollectionPropertyAccessor"/>
    <bean type="ognl.PropertyAccessor" userName="java.util.Map"
          class="com.opensymphony.xwork2.ognl.accessor.XWorkMapPropertyAccessor"/>
    <bean type="ognl.PropertyAccessor" userName="java.util.Collection"
          class="com.opensymphony.xwork2.ognl.accessor.XWorkCollectionPropertyAccessor"/>
    <bean type="ognl.PropertyAccessor" userName="com.opensymphony.xwork2.ognl.ObjectProxy"
          class="com.opensymphony.xwork2.ognl.accessor.ObjectProxyPropertyAccessor"/>
    <bean type="ognl.PropertyAccessor" userName="org.apache.struts2.dispatcher.HttpParameters"
          class="com.opensymphony.xwork2.ognl.accessor.HttpParametersPropertyAccessor"/>
    <bean type="ognl.PropertyAccessor" userName="org.apache.struts2.dispatcher.Parameter"
          class="com.opensymphony.xwork2.ognl.accessor.ParameterPropertyAccessor"/>

    <bean type="ognl.MethodAccessor" userName="java.lang.Object"
          class="com.opensymphony.xwork2.ognl.accessor.XWorkMethodAccessor"/>
    <bean type="ognl.MethodAccessor" userName="com.opensymphony.xwork2.util.CompoundRoot"
          class="com.opensymphony.xwork2.ognl.accessor.CompoundRootAccessor"/>

    <bean class="org.apache.struts2.views.jsp.ui.OgnlTool"/>

    <bean type="org.apache.struts2.dispatcher.StaticContentLoader"
          class="org.apache.struts2.dispatcher.DefaultStaticContentLoader" userName="struts"/>
    <bean type="com.opensymphony.xwork2.UnknownHandlerManager"
          class="com.opensymphony.xwork2.DefaultUnknownHandlerManager" userName="struts"/>

    <bean type="org.apache.struts2.dispatcher.DispatcherErrorHandler" userName="struts"
          class="org.apache.struts2.dispatcher.DefaultDispatcherErrorHandler"/>

    <constant userName="struts.dispatcher.errorHandler" value="struts"/>
    <bean type="ognl.PropertyAccessor" userName="java.util.ArrayList"
          class="com.opensymphony.xwork2.ognl.accessor.XWorkListPropertyAccessor"/>
    <bean type="ognl.PropertyAccessor" userName="java.util.HashSet"
          class="com.opensymphony.xwork2.ognl.accessor.XWorkCollectionPropertyAccessor"/>
    <bean type="ognl.PropertyAccessor" userName="java.util.HashMap"
          class="com.opensymphony.xwork2.ognl.accessor.XWorkMapPropertyAccessor"/>

    <bean type="com.opensymphony.xwork2.security.ExcludedPatternsChecker" userName="struts"
          class="com.opensymphony.xwork2.security.DefaultExcludedPatternsChecker" scope="prototype"/>
    <bean type="com.opensymphony.xwork2.security.AcceptedPatternsChecker" userName="struts"
          class="com.opensymphony.xwork2.security.DefaultAcceptedPatternsChecker" scope="prototype"/>

    <bean type="com.opensymphony.xwork2.config.providers.ValueSubstitutor"
          class="com.opensymphony.xwork2.config.providers.EnvsValueSubstitutor" scope="singleton"/>
    <!--用户编写的package(struts.xml)必须要继承struts-default包-->
    <package userName="struts-default" abstract="true" strict-method-invocation="true">
        <!--result-types定义了结果的跳转类型
            dispatcher：转发，默认
            redirect：重定向
            redirectAction：重定向到另一个action资源
            stream(文件下载时使用)
        -->
        <result-types>
            <result-type userName="chain" class="com.opensymphony.xwork2.ActionChainResult"/>
            <result-type userName="dispatcher" class="org.apache.struts2.result.ServletDispatcherResult" default="true"/>
            <result-type userName="freemarker" class="org.apache.struts2.views.freemarker.FreemarkerResult"/>
            <result-type userName="httpheader" class="org.apache.struts2.result.HttpHeaderResult"/>
            <result-type userName="redirect" class="org.apache.struts2.result.ServletRedirectResult"/>
            <result-type userName="redirectAction" class="org.apache.struts2.result.ServletActionRedirectResult"/>
            <result-type userName="stream" class="org.apache.struts2.result.StreamResult"/>
            <result-type userName="velocity" class="org.apache.struts2.result.VelocityResult"/>
            <result-type userName="xslt" class="org.apache.struts2.views.xslt.XSLTResult"/>
            <result-type userName="plainText" class="org.apache.struts2.result.PlainTextResult"/>
            <result-type userName="postback" class="org.apache.struts2.result.PostbackResult"/>
        </result-types>

        <!--定义了所有的拦截器-->
        <!--
            拦截器功能与过滤器功能类似，拦截器只拦截action请求
        -->
        <interceptors>
            <interceptor userName="alias" class="com.opensymphony.xwork2.interceptor.AliasInterceptor"/>
            <interceptor userName="autowiring"
                         class="com.opensymphony.xwork2.spring.interceptor.ActionAutowiringInterceptor"/>
            <interceptor userName="chain" class="com.opensymphony.xwork2.interceptor.ChainingInterceptor"/>
            <interceptor userName="conversionError"
                         class="org.apache.struts2.interceptor.StrutsConversionErrorInterceptor"/>
            <interceptor userName="cookie" class="org.apache.struts2.interceptor.CookieInterceptor"/>
            <interceptor userName="cookieProvider" class="org.apache.struts2.interceptor.CookieProviderInterceptor"/>
            <interceptor userName="clearSession" class="org.apache.struts2.interceptor.ClearSessionInterceptor"/>
            <interceptor userName="createSession" class="org.apache.struts2.interceptor.CreateSessionInterceptor"/>
            <interceptor userName="debugging" class="org.apache.struts2.interceptor.debugging.DebuggingInterceptor"/>
            <interceptor userName="execAndWait" class="org.apache.struts2.interceptor.ExecuteAndWaitInterceptor"/>
            <interceptor userName="exception" class="com.opensymphony.xwork2.interceptor.ExceptionMappingInterceptor"/>
            <interceptor userName="fileUpload" class="org.apache.struts2.interceptor.FileUploadInterceptor"/>
            <interceptor userName="i18n" class="org.apache.struts2.interceptor.I18nInterceptor"/>
            <interceptor userName="logger" class="com.opensymphony.xwork2.interceptor.LoggingInterceptor"/>
            <interceptor userName="modelDriven" class="com.opensymphony.xwork2.interceptor.ModelDrivenInterceptor"/>
            <interceptor userName="scopedModelDriven"
                         class="com.opensymphony.xwork2.interceptor.ScopedModelDrivenInterceptor"/>
            <interceptor userName="params" class="com.opensymphony.xwork2.interceptor.ParametersInterceptor"/>
            <interceptor userName="paramRemover" class="com.opensymphony.xwork2.interceptor.ParameterRemoverInterceptor"/>
            <interceptor userName="actionMappingParams"
                         class="org.apache.struts2.interceptor.ActionMappingParametersInterceptor"/>
            <interceptor userName="prepare" class="com.opensymphony.xwork2.interceptor.PrepareInterceptor"/>
            <interceptor userName="staticParams" class="com.opensymphony.xwork2.interceptor.StaticParametersInterceptor"/>
            <interceptor userName="scope" class="org.apache.struts2.interceptor.ScopeInterceptor"/>
            <interceptor userName="servletConfig" class="org.apache.struts2.interceptor.ServletConfigInterceptor"/>
            <interceptor userName="timer" class="com.opensymphony.xwork2.interceptor.TimerInterceptor"/>
            <interceptor userName="token" class="org.apache.struts2.interceptor.TokenInterceptor"/>
            <interceptor userName="tokenSession" class="org.apache.struts2.interceptor.TokenSessionStoreInterceptor"/>
            <interceptor userName="validation"
                         class="org.apache.struts2.interceptor.validation.AnnotationValidationInterceptor"/>
            <interceptor userName="workflow" class="com.opensymphony.xwork2.interceptor.DefaultWorkflowInterceptor"/>
            <interceptor userName="store" class="org.apache.struts2.interceptor.MessageStoreInterceptor"/>
            <interceptor userName="checkbox" class="org.apache.struts2.interceptor.CheckboxInterceptor"/>
            <interceptor userName="datetime" class="org.apache.struts2.interceptor.DateTextFieldInterceptor"/>
            <interceptor userName="profiling" class="org.apache.struts2.interceptor.ProfilingActivationInterceptor"/>
            <interceptor userName="roles" class="org.apache.struts2.interceptor.RolesInterceptor"/>
            <interceptor userName="annotationWorkflow"
                         class="com.opensymphony.xwork2.interceptor.annotations.AnnotationWorkflowInterceptor"/>
            <interceptor userName="annotationParameterFilter"
                         class="com.opensymphony.xwork2.interceptor.annotations.AnnotationParameterFilterInterceptor"/>
            <interceptor userName="multiselect" class="org.apache.struts2.interceptor.MultiselectInterceptor"/>
            <interceptor userName="noop" class="org.apache.struts2.interceptor.NoOpInterceptor"/>

            <!--定义拦截器栈
                为了拦截器引用方便，引用了拦截器栈的概念
            -->
            <interceptor-stack userName="emptyStack">
                <interceptor-ref userName="noop"/>
            </interceptor-stack>


            <interceptor-stack userName="basicStack">
                <interceptor-ref userName="exception"/>
                <interceptor-ref userName="servletConfig"/>
                <interceptor-ref userName="prepare"/>
                <interceptor-ref userName="checkbox"/>
                <interceptor-ref userName="datetime"/>
                <interceptor-ref userName="multiselect"/>
                <interceptor-ref userName="actionMappingParams"/>
                <interceptor-ref userName="params"/>
                <interceptor-ref userName="conversionError"/>
            </interceptor-stack>


            <interceptor-stack userName="validationWorkflowStack">
                <interceptor-ref userName="basicStack"/>
                <interceptor-ref userName="validation"/>
                <interceptor-ref userName="workflow"/>
            </interceptor-stack>


            <interceptor-stack userName="fileUploadStack">
                <interceptor-ref userName="fileUpload"/>
                <interceptor-ref userName="basicStack"/>
            </interceptor-stack>


            <interceptor-stack userName="modelDrivenStack">
                <interceptor-ref userName="modelDriven"/>
                <interceptor-ref userName="basicStack"/>
            </interceptor-stack>


            <interceptor-stack userName="chainStack">
                <interceptor-ref userName="chain"/>
                <interceptor-ref userName="basicStack"/>
            </interceptor-stack>

            <interceptor-stack userName="i18nStack">
                <interceptor-ref userName="i18n"/>
                <interceptor-ref userName="basicStack"/>
            </interceptor-stack>


            <interceptor-stack userName="paramsPrepareParamsStack">
                <interceptor-ref userName="exception"/>
                <interceptor-ref userName="alias"/>
                <interceptor-ref userName="i18n"/>
                <interceptor-ref userName="checkbox"/>
                <interceptor-ref userName="datetime"/>
                <interceptor-ref userName="multiselect"/>
                <interceptor-ref userName="params"/>
                <interceptor-ref userName="servletConfig"/>
                <interceptor-ref userName="prepare"/>
                <interceptor-ref userName="chain"/>
                <interceptor-ref userName="modelDriven"/>
                <interceptor-ref userName="fileUpload"/>
                <interceptor-ref userName="staticParams"/>
                <interceptor-ref userName="actionMappingParams"/>
                <interceptor-ref userName="params"/>
                <interceptor-ref userName="conversionError"/>
                <interceptor-ref userName="validation">
                    <param userName="excludeMethods">input,back,cancel,browse</param>
                </interceptor-ref>
                <interceptor-ref userName="workflow">
                    <param userName="excludeMethods">input,back,cancel,browse</param>
                </interceptor-ref>
            </interceptor-stack>

            <!--默认执行的拦截器：18个，按顺序执行
            用户访问时按顺序执行18个拦截器
            先执行Action类的创建，再执行拦截器，最后执行业务方法
            -->
            <interceptor-stack userName="defaultStack">
                <interceptor-ref userName="exception"/>
                <interceptor-ref userName="alias"/>
                <interceptor-ref userName="servletConfig"/>
                <interceptor-ref userName="i18n"/>
                <interceptor-ref userName="prepare"/>
                <interceptor-ref userName="chain"/>
                <interceptor-ref userName="scopedModelDriven"/>
                <interceptor-ref userName="modelDriven"/>
                <interceptor-ref userName="fileUpload"/>
                <interceptor-ref userName="checkbox"/>
                <interceptor-ref userName="datetime"/>
                <interceptor-ref userName="multiselect"/>
                <interceptor-ref userName="staticParams"/>
                <interceptor-ref userName="actionMappingParams"/>
                <interceptor-ref userName="params"/>
                <interceptor-ref userName="conversionError"/>
                <interceptor-ref userName="validation">
                    <param userName="excludeMethods">input,back,cancel,browse</param>
                </interceptor-ref>
                <interceptor-ref userName="workflow">
                    <param userName="excludeMethods">input,back,cancel,browse</param>
                </interceptor-ref>
                <interceptor-ref userName="debugging"/>
            </interceptor-stack>


            <interceptor-stack userName="completeStack">
                <interceptor-ref userName="defaultStack"/>
            </interceptor-stack>

            <interceptor-stack userName="executeAndWaitStack">
                <interceptor-ref userName="execAndWait">
                    <param userName="excludeMethods">input,back,cancel</param>
                </interceptor-ref>
                <interceptor-ref userName="defaultStack"/>
                <interceptor-ref userName="execAndWait">
                    <param userName="excludeMethods">input,back,cancel</param>
                </interceptor-ref>
            </interceptor-stack>

        </interceptors>
        <!--定义了所有的拦截器-->

        <!--默认执行的拦截器栈-->
        <default-interceptor-ref userName="defaultStack"/>
        <!--默认执行的action-->
        <default-class-ref class="com.opensymphony.xwork2.ActionSupport"/>

        <global-allowed-methods>execute,input,back,cancel,browse,save,delete,list,index</global-allowed-methods>

    </package>

</struts>
